=> m(count, \"Спонсор\", \"Спонсора\", \"Спонсоров\")",{"type":22,"value":38},"Светлая",{"type":22,"value":40},"Категория",{"type":22,"value":42},"Поиск",{"type":22,"value":44},"Материалы",{"type":22,"value":46},"Ключевые элементы",{"type":22,"value":48},"Статистика",{"type":22,"value":50},"Темная",{"type":22,"value":30},{"type":22,"value":53},"Языки",{"type":22,"value":55},"Системная",{"type":22,"value":57},"Статья",{"type":22,"value":59},"Настройки",{"type":22,"value":61},"Собрано",{"type":22,"value":63},"Конспект",{"type":22,"value":65},"Контент",{"type":22,"value":67},"Практика",{"type":22,"value":69},"Erudit",{"type":22,"value":71},"Вы собираетесь перейти на страницу контента:",{"type":22,"value":73},"Добавить перевод",{"type":22,"value":75},"Главная страница",{"type":22,"value":77},"Авторы",{"type":22,"value":79},"Спонсоры",{"type":22,"value":81},"Поиск по сайту...",{"type":22,"value":83},"Ищем еще...",{"type":22,"value":85},"Ничего не найдено",{"type":22,"value":87},"Больше результатов нет",{"type":22,"value":89},"Контента нет.",{"type":22,"value":91},"К оглавлению",{"type":22,"value":93},"Об учебнике",{"type":22,"value":95},"Разработка",{"type":22,"value":97},"Профиль",{"type":22,"value":99},"Дополнение",{"type":22,"value":101},"Редактор",{"type":27,"value":103},"(name) => `Персональная страница автора \"${name}\" с данными о вкладе в проект.`",{"type":22,"value":105},"Список людей и организаций, которые поддерживают проект финансово. Благодаря им проект может существовать и развиваться. Если вы хотите помочь проекту, то тоже можете стать спонсором и попасть на эту страницу!",{"type":22,"value":107},"Список людей, которые внесли вклад в материалы проекта: предложили ценные идеи, вносили корректировки в существующий материал или же написали собственный!",{"type":22,"value":109},"Стать спонсором",{"type":22,"value":111},"Вы тоже можете помочь проекту, внести свой вклад и попасть на эту страницу!",{"type":22,"value":113},"Пока что спонсоров нет. Вы можете стать первым!",{"type":22,"value":115},"Стать автором",{"type":22,"value":117},"Пока что авторов нет. Вы можете стать первым!",{"type":22,"value":119},"Вклад",{"type":27,"value":121},"(contentTitle) => `Понятное и интересное объяснение темы «${contentTitle}». Показательные примеры, важные свойства, интересные факты, применение в жизни, понятные доказательства. Здесь вы точно разберетесь!`",{"type":22,"value":123},"Начать изучение!",{"type":27,"value":125},"(contentTitle) => `Конспект темы «${contentTitle}»: ключевые определения, теоремы, свойства и примеры их использвания. Все самое важное и в кратком виде!`",{"type":22,"value":127},"Связи",{"type":27,"value":129},"(contentTitle) => `Разнообразные задачи с подсказками и ответами по теме «${contentTitle}». Интересные условия, подсказки и подробные решения. Превратите знания в навык!`",{"type":22,"value":131},"Надо знать",{"type":22,"value":97},{"type":22,"value":134},"Зависит от",{"type":22,"value":136},"Этот материал предназначен для учеников с высоким уровнем понимания предмета. Информация здесь не предназначена для новичков!",{"type":22,"value":138},"Используется в",{"type":22,"value":95},{"type":22,"value":141},"Внешние",{"type":22,"value":143},"Этот материал в разработке! Он может измениться в будущем и даже содержать ошибки!",{"type":22,"value":99},{"type":22,"value":146},"Это дополнительный материал для тех, кто хочет глубже погрузиться предмет и получить дополнительные знания и контекст.",{"m":148},"function m(number, one, two, five, includeNumber = true) {\n  const text = [five, one, two, two, two, five][number % 100 > 10 && number % 100 \u003C 20 ? 0 : Math.min(number % 10, 5)];\n  return includeNumber ? `${number} ${text}` : text;\n}",{"contentPath":150,"mainContent":151},"book/basics/",{"fullId":5,"shortId":5,"contentRelativePath":152,"title":15,"bookTitle":15,"breadcrumbs":153,"description":154,"stats":155,"connections":161,"contributions":177,"type":14,"children":190},"01-basics",[],"\n    Все стартовые и самые полезные темы, формулы и понятия, на основе которых строится вся математика.\n    Идеальная стартовая точка, если полный чайник в математике.\n    Предварительных знаний не требуется.\n    Всё расписано простым и понятным языком.\n  ",{"materials":156,"elements":157},3,{"accent_term":158,"accent_statement":159,"accent_important":158,"problem":160},6,4,48,{"externals":162},[163],{"type":164,"items":165},"own",[166,172],{"type":167,"title":168,"info":169,"link":170,"reason":171},"web","LibreTexts Mathematics","Открытая и бесплатная онлайн-коллекция учебных материалов по математике.","https://math.libretexts.org/","Большое количество материалов и много типовых задач на отработку.",{"type":173,"title":174,"info":175,"reason":176},"physical","Математика. 6 класс","Мерзляк А.Г., Полонский В.Б., Якир М.С.","Огромное количество самых разных задач, от элементарнейших до довольно хитрых.",[178,182,186],{"contributorId":179,"name":180,"avatarUrl":181},"gwynerva","Пётр Радько","/file/contributors/gwynerva/avatar.png",{"contributorId":183,"name":184,"avatarUrl":185},"nagibin","Николай Нагибин","/file/contributors/nagibin/avatar.jpg",{"contributorId":187,"name":188,"avatarUrl":189},"igor-radko","Игорь Радько","/file/contributors/igor-radko/avatar.jpg",[191],{"type":192,"link":193,"title":194,"description":195,"stats":196},"group","/group/basics/equations/","Уравнения","\n    Разбор самых популярных уравнений: элементарных, квадратных, систем и т.д.\n    Вы поймёте, зачем их вообще нужно решать и как с их помощью можно описывать процессы в реальной жизни.\n    Разберем и нюансы: преобразования, ОДЗ, \"потеря\" и появление \"лишних\" корней и многие другие.\n  ",{"materials":156,"elements":197},{"accent_term":158,"accent_statement":159,"accent_important":158,"problem":160},{"contributorsCount":199,"sponsorsCount":200},7,5,{"shortBookId":5,"frontNav":202},{"type":14,"shortId":5,"title":15,"flags":203,"link":17,"children":204},{},[205],{"type":192,"separator":206,"shortId":207,"t